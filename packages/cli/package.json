{
  "name": "stackpatch",
  "version": "1.2.9",
  "description": "Composable frontend features for modern React & Next.js apps - Add authentication, UI components, and more with zero configuration",
  "main": "dist/stackpatch.js",
  "bin": {
    "stackpatch": "dist/stackpatch.js",
    "create-stackpatch": "dist/stackpatch.js"
  },
  "type": "module",
  "files": [
    "dist",
    "boilerplate",
    "README.md"
  ],
  "scripts": {
    "install-deps": "pnpm install @rollup/plugin-node-resolve @rollup/plugin-commonjs @rollup/plugin-typescript @rollup/plugin-json rollup typescript -D",
    "build": "rollup -c rollup.config.cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:cli": "node scripts/test-cli-execution.js",
    "test:nextjs-version": "node scripts/test-nextjs-version.js",
    "prepublishOnly": "npm run build && node scripts/prepare-publish.js && npm run test:cli",
    "dev": "bun run bin/stackpatch.ts",
    "create": "bun run bin/stackpatch.ts"
  },
  "keywords": [
    "nextjs",
    "react",
    "boilerplate",
    "cli",
    "frontend",
    "stackpatch",
    "next-auth",
    "authentication",
    "scaffolding",
    "generator"
  ],
  "author": {
    "name": "Darshit Shukla",
    "url": "https://github.com/Darshh09"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Darshh09/StackPatch.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/Darshh09/StackPatch/issues"
  },
  "homepage": "https://github.com/Darshh09/StackPatch#readme",
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "packageManager": "pnpm@10.28.0",
  "dependencies": {
    "@inquirer/select": "^5.0.4",
    "chalk": "^5.6.2",
    "fs-extra": "^11.3.3",
    "inquirer": "^13.2.0",
    "jimp": "^0.22.10",
    "tsx": "^4.19.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.9",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.0.0",
    "rollup": "^4.55.1",
    "typescript": "^5.9.3",
    "vitest": "^2.1.0"
  }
}
